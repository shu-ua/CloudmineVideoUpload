# platform :ios, '9.0'

target 'CloudmineVideoUploading' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'AFNetworking'
  pod 'CloudMine' , :git => 'https://github.com/shu-ua/CloudMineSDK-iOS.git'
